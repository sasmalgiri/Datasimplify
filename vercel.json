{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/refund-policy",
      "destination": "/refund",
      "permanent": true
    },
    {
      "source": "/template-setup",
      "destination": "/template-requirements",
      "permanent": true
    },
    {
      "source": "/setup",
      "destination": "/template-requirements",
      "permanent": true
    },
    {
      "source": "/requirements",
      "destination": "/template-requirements",
      "permanent": true
    },
    {
      "source": "/safecontract",
      "destination": "/smart-contract-verifier",
      "permanent": true
    },
    {
      "source": "/contract",
      "destination": "/smart-contract-verifier",
      "permanent": true
    },
    {
      "source": "/whales",
      "destination": "/coming-soon",
      "permanent": false
    },
    {
      "source": "/exchanges",
      "destination": "/coming-soon",
      "permanent": false
    },
    {
      "source": "/nft",
      "destination": "/coming-soon",
      "permanent": false
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup/run",
      "schedule": "0 5 * * *"
    },
    {
      "path": "/api/cron/export",
      "schedule": "*/15 * * * *"
    }
  ]
}
