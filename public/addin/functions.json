  "functions": [
    {
      "id": "PRICE",
      "name": "PRICE",
      "description": "Get current price of a cryptocurrency",
      "parameters": [
        {
          "name": "coin",
          "description": "Coin ID (e.g., bitcoin, ethereum)",
          "type": "string"
        },
        {
          "name": "currency",
          "description": "Currency (default: usd)",
          "type": "string",
          "optional": true
        }
      ],
      "result": {
        "type": "number"
      },
      "options": {
        "cancelable": false,
        "requiresAddress": false,
        "stream": false,
        "volatile": false
      }
